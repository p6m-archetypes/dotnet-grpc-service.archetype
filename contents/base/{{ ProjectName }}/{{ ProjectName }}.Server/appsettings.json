{
  "ConnectionStrings": {
    "CockroachDBConnection": "Host=localhost;Port={{ database-port }};Database={{ project-name }}-db;Username=root;Password="
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://localhost:8080",
        "Protocols": "Http2"
      },
      "Http": {
        "Url": "http://localhost:8081",
        "Protocols": "Http1"
      }
    }
  }
}
